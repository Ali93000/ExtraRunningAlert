{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "PrayerTime": {
    "ApiURL": "http://api.aladhan.com/v1/timingsByCity?",
    "City": "Riyadh",
    "Country": "SaudiArabia",
    "TimeBeforSalah": "15",
    "TimeAfterSalah25": "25",
    "TimeAfterSalah30": "30",
    "MidNightTime": "11:45",
    "SalahVoice": "D:\\DoniaGedida.mp3",
    "AzanVoice": "D:\\DoniaGedida.mp3",
    "AfterSalah25Minutes": "D:\\DoniaGedida.mp3",
    "AfterSalah30Minutes": "D:\\DoniaGedida.mp3",
    "MidNightTimePath": "D:\\DoniaGedida.mp3"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": { "path": "Xlog.txt" }
      },
      { "Name": "Console" }
    ]
  }
}
